{
  "name": "test-now-next-app",
  "scripts": {
    "now:dev": "now dev",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "yarn eslint . --ext .ts,.tsx",
    "deploy:staging": "now -A staging.now.json --target=staging --scope=mthomps4 && now alias -A staging.now.json",
    "deploy:prod": "now -A prod.now.json --target=production --scope=mthomps4 && now alias -A prod.now.json",
    "ci:deploy:staging": "now -A staging.now.json --target=staging --token=$NOW_TOKEN --scope=mthomps4  && now alias -A staging.now.json  --token=$NOW_TOKEN",
    "ci:deploy:prod": "now -A prod.now.json --target=production --token=$NOW_TOKEN --scope=mthomps4 && now alias -A prod.now.json  --token=$NOW_TOKEN"
  },
  "dependencies": {
    "next": "^9.0.2",
    "now": "^16.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2",
    "typeorm": "^0.2.18"
  },
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@now/node": "^1.0.0",
    "@types/next": "^8.0.6",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/styled-components": "^4.1.19",
    "@types/styled-jsx": "^2.2.8",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-styled-components": "^1.10.6",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
